package odd_even_prog;

public class Prime {
	public static void main(String[] args) {
		int no=5;
		boolean flag = true;
		for(int i=2;i<=no;i++)
		{
			if(no%2==0)
			{
				flag=true;
				break;
			}
		}
		if(flag==false)
		{
			System.out.println("prime no "+no);
		}else{
			System.out.println("not prime                             "+no);
		}
	}

}
