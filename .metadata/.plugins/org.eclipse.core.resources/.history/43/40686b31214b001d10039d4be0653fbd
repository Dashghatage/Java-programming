package practiccc;
public class Tester {	
public static void main(String[] args) {
	int no=153;
	int copy=no;
	int sum=0;
	
	while (no!=0) {
	  int rem=no%10;
	  sum+=rem*rem*rem;
	  no=no/10;
	}
	if (sum==copy) {
		System.out.println("it is strong number");
	} else {
     System.out.println("it is not strong");
	}
}
}
