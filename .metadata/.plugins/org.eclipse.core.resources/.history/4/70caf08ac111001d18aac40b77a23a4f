package Upcastingtopic;
class Tester
{
	int r=15;
}
class Tista extends Tester
{
	void utlliya()
	{
		System.out.println("hey utlliya");
	}
}
public class Mainclass3 {
	public static void main(String[] args) {
		Tester t1=new Tista();
		System.out.println(t1.r);
		Tista t2=(Tista)t1;
		System.out.println(t2.r);
		t2.r();
	}

}
