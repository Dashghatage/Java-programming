package school;

public class Monday {
	public static void main(String[] args) {
		for (int i = 2; i <= 100; i++) {
			boolean flag = true;
			int no = i;
			for (int j = 2; j < no; j++) {
				if (no % j == 0) {
					flag = false;
					break;
				}
			}
			if (flag == true) {
				System.out.println("prime number: " + no);
			}
		}
	}

}