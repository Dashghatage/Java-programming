package odd_even_prog;

public class RangePrime {
	public static void main(String[] args) {
		for(int i=2;i<=100;i++)
		{
			int j;
			int no=j;
			boolean flag=true;
			for(int j1=1;j1<=1;j1++)
			{
				if((no%2)==0)
				{
					flag=false;
					break;
				}
			}
			System.out.println("prime no "+no);
		}
	}

}
