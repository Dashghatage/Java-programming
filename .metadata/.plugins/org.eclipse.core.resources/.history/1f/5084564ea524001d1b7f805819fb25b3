package Armstrong;

import java.util.Scanner;

public class Strong {
public static void main(String[] args, Object fact) {
	Scanner sc=new Scanner(System.in);
	System.out.println("enter numb");
	int no=sc.nextInt();
	int copy=no;
	int sum=0;
	while(no!=0)
	{
		int rem=no%10;
		int fact1=1;
	}
	for(int i=rem;i>0;i--)
	{
		fact=fact*i;
	}
	sum=sum+fact;
	no=no/10;
	if(copy==sum)
	{
		System.out.println("it is Strong number");
	}else{
		System.out.println("it is not Strong number");
	}
}
}
