package odd_even_prog;

public class RangePrime {
	public static void main(String[] args) {
		for(int i=2;i<=100;i++)
		{
			int no=5;
			boolean flag=true;
			for(int j=1;j<=50;j++)
			{
				if(no%2==0)
				{
					flag=false;
					break;
				}
			}
		}
		boolean flag;
		if(flag==true)
		{
			System.out.println("prime no "+no);
		}
	}

}
