package practiccc;

import java.util.Scanner;

public class Tester {	
public static void main(String[] args) {
	int no=28;
	int copy=no;
	int sum=0;
	int i=1;
	
	while (i<=no/2) {
		if(no%i==0){
			sum+=i;
			i++;
		}
	}
	if (sum==copy) {
		System.out.println("it is perfect no");
	} else {
  System.out.println("it is not perfect");
	}
}
}
